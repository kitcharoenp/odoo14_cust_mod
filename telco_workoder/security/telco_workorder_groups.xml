<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="group_telco_workorder_officer" model="res.groups">
        <field name="name">Telco WorkOrder Officer</field>
        <field name="category_id" ref="base.module_category_hidden"/>
        <field name="implied_ids"
            eval="[(4, ref('base.group_user'))]"/>
        <field name="comment">the user will be able to create document.</field>
        <field name="users" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_telco_workorder_manager" model="res.groups">
        <field name="name">Telco WorkOrder Manager</field>
        <field name="category_id" ref="base.module_category_hidden"/>
        <field name="implied_ids"
            eval="[(4, ref('group_telco_workorder_officer'))]"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

</odoo>
